while True:
    print("\nWelcome to learning AI 101:")
    print("1. Register")
    print("2. Registration Slip")
    print("3. Exit")
    choice = input("Enter choice: ")
    if choice == "1":
        try:
            name = (input("Enter your name: ")) # collects user name
            if (len(name) == 0) or (name.isdigit() == True):
                raise ValueError("Invalid Input, Enter a name.")
        except ValueError as e:
                print("Error: ", e)
        
        try:
            age = int(input("Enter your age: ")) # collects user name
        except ValueError:
            print("Invalid Input, must be a number.")
                
        try:
            phone = input("Enter Phone Num: ")
            if len(phone) != 11:
                raise ValueError("Invalid Input, Enter a valid number.")
        except ValueError as e:
                print("Error: ", e)

        try:
            track = input("Enter Track: ")
            if (len(track) == 0) or (track.isdigit() == True):
                raise ValueError("Invalid Input, Track can\'t be empty")
        except ValueError as e:
                print("Error: ", e)
           